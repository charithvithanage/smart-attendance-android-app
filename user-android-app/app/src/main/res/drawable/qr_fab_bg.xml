<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_primary"></solid>
            <size
                android:width="@dimen/logout_button_size"
                android:height="@dimen/logout_button_size" />
            <corners android:radius="@dimen/logout_button_size" />
            <stroke
                android:width="1dp"
                android:color="@color/black" />
        </shape>
    </item>
    <item
        android:width="20dp"
        android:height="20dp"
        android:drawable="@drawable/qr_scan_icon"
        android:gravity="center"></item>
</layer-list>

